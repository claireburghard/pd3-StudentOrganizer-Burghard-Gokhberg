import java.io.*;
import java.util.*;

public class User{
    private String username = "";
    Scanner sc = new Scanner(System.in);
    
    public void welcome(){
	if (username.equals("")){
	    System.out.println("Welcome to your Student Organizer! Enter a new username!");
	    username = sc.nextLine();
	}else{
	    System.out.println("Welcome back " + username);
	}
    }

    
